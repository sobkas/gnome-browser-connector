{
  "applications": {
    "gecko": {
      "id": "chrome-gnome-shell@gnome.org",
      "strict_min_version": "52.0"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "nativeMessaging",
    "notifications",
    "storage",
    "tabs",
    "https://extensions.gnome.org/"
  ]
}
